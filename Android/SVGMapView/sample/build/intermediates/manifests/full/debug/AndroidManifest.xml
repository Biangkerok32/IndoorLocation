<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.jiahuan.svgmapview.sample"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="22" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity android:name="com.jiahuan.svgmapview.sample.SplashActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.jiahuan.svgmapview.sample.LocationOverlayActivity" />
        <activity android:name="com.jiahuan.svgmapview.sample.SparkActivity" />
        <activity android:name="com.jiahuan.svgmapview.sample.MainActivity" />
        <activity android:name="com.jiahuan.svgmapview.sample.SparkStepActivity" />
    </application>

</manifest>